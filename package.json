{
  "name": "perf-lab",
  "version": "1.0.0",
  "description": "Node.js Performance Lab",
  "main": "dist/index.js",
  "scripts": {
    "benchmark": "tsc && node dist/index.js",
    "benchmark:quick": "tsc && node dist/quick/index.js",
    "benchmark:loops": "tsc && node dist/quick/loop-benchmark.js",
    "benchmark:async": "tsc && node dist/quick/async-loops.js",
    "benchmark:data": "tsc && node dist/quick/data-structures.js",
    "benchmark:strings": "tsc && node dist/quick/string-operations.js",
    "benchmark:functions": "tsc && node dist/quick/function-calls.js",
    "benchmark:io": "tsc && node dist/io/benchmarks.js",
    "benchmark:memory": "tsc && node dist/memory/benchmarks.js",
    "benchmark:profile": "tsc && node dist/profiling/tools.js",
    "benchmark:advanced": "tsc && node dist/benchmark/runner.js",
    "build": "tsc",
    "clean": "rm -rf dist results",
    "watch": "tsc --watch",
    "start": "npm run benchmark"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^25.0.9",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}